<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.ma.pictureeditdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".GalleryApplication"
        android:theme="@style/Theme.PictureEditDemo"
        tools:ignore="MissingClass">
        <activity
            android:name=".TouchActivity"
            android:exported="false" />
        <activity
            android:name=".CropActivity"
            android:exported="false" />
        <activity
            android:name=".RulerActivity"
            android:exported="false" />
        <activity
            android:name=".ScalerRotateActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FullscreenActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.PictureEditDemo.Fullscreen" />
        <activity android:name=".temp.CropImageActivity" />
    </application>

</manifest>